@model CWAC19AcluMo.ViewModels.SendNotificationViewModel
@{
    ViewData["Title"] = "SendStatusUpdate";
}

<h2>SendStatusUpdate</h2>

<form method="post" asp-controller="StatusUpdates" asp-for="SendStatusUpdate">
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="StatusCode"></label>
        <input asp-for="StatusCode" class="form-control" />
    </div>
    <input type="submit" value="Submit" />
</form>

